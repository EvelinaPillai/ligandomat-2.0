<!DOCTYPE html>

<html metal:use-macro="load: base_layout.pt">
<html lang="${request.locale_name}">
  <head>

  </head>

  <body>
  <div class="main_wrapper">
  <div style="margin-top:20pt">

    <table id="example" class="info_table" cellspacing="0" width="100%"> <thead>
            <tr>
                <th>Name</th>
                <th>Organ</th>
                <th>Dignity</th>
                <th>Celltype</th>
                <th>Histology</th>
                <th>Location</th>
                <th>Metastatis</th>
                <th>Organism</th>
                <th>Person</th>
            </tr>
        </thead></table>
    </div>
</div>




    <script>

    $(document).ready(function() {

    $('#example').dataTable( {
        data: ${project},
        columns: [
            { data: "name" ,
            "render": function ( data, type, row ) {
                return "<a class='nostylelink'  href= '/source/"+data + "'>"+data + "</a>" ;
            }},
            { data: "organ",
            "render": function ( data, type, row ) {
                return "<a class='nostylelink'  href= '/organ/"+data + "'>"+data + "</a>" ;
            }},
            { data: "dignity" },
            { data: "celltype"},
            { data: "histology",
            "render": function ( data, type, row ) {
                return "<a class='nostylelink'  href= '/dignity/"+data + "'>"+data + "</a>" ;
            }},
            { data: "location"},
            { data: "metastatis"},
            { data: "organism"},
            { data: "person",
            "render": function ( data, type, row ) {
                return "<a class='nostylelink'  href= '/person/"+data + "'>"+data + "</a>" ;
            }}
        ],
        "sDom": 'T<"clear">lfrtip',
        oTableTools: {
            "sSwfPath": "${request.static_url('ligando:static/swf/copy_csv_xls_pdf.swf')}",
            "aButtons":  ["csv"]
        }
    } );
    } );
    </script>



  </body>
</html>
