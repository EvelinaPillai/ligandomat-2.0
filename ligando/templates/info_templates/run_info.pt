<!DOCTYPE html>

<html metal:use-macro="load: ../base_layout.pt">
<html lang="${request.locale_name}">
  <head>

  </head>

  <body>
  <div class="main_wrapper">
    <div style="margin-top:20pt">
    <table id="example" class="info_table" cellspacing="0" > <thead>
            <tr>
                <th>Name</th>
                <th>Source Name</th>
                <th>Organ</th>
                <th>Dignity</th>
                <th>Date</th>
                <th>Antibody</th>
                <th>Preperation Comment</th>
                <th>Used Share</th>
                <th>Sample Mass</th>
            </tr>
        </thead></table>

    </div>
</div>
       <script>

    $(document).ready(function() {
    // create dataTable
    $('#example').dataTable( {
        data: ${project},
        columns: [
            { data: "filename" ,
            "render": function ( data, type, row ) {
                return "<a class='nostylelink'  href= '/msrun/"+row["ms_run_id"] + "'>"+data + "</a>" ;
            }},
            { data: "name" ,
            "render": function ( data, type, row ) {
                return "<a class='nostylelink'  href= '/source/"+data + "'>"+data + "</a>" ;
            }},
            { data: "organ",
            "render": function ( data, type, row ) {
                return "<a class='nostylelink'  href= '/organ/"+data + "'>"+data + "</a>" ;
            }},
            { data: "dignity" },
            { data: "ms_run_date"},
            { data: "antibody_set"},
            { data: "prep_comment" },
            { data: "used_share"},
            { data: "sample_mass"}
        ],
        "sDom": 'T<"clear">lfrtip',
        oTableTools: {
            "sSwfPath": "${request.static_url('ligando:static/swf/copy_csv_xls_pdf.swf')}",
            "aButtons":  ["csv"]
        }
    } );
    } );
    </script>

  </body>
</html>
